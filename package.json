{
  "name": "gesture-app-client",
  "description": "Sample app for visualizing and training hand gestures",
  "keywords": [
    "handpose",
    "tensorflowJS"
  ],
  "version": "1.0.0",
  "proxy": "http://localhost:4000",
  "dependencies": {
    "@fontsource/roboto": "^4.4.5",
    "@tensorflow-models/handpose": "^0.0.7",
    "@tensorflow-models/posenet": "^2.2.2",
    "@tensorflow/tfjs": "^3.7.0",
    "@tensorflow/tfjs-backend-webgl": "^3.7.0",
    "@tensorflow/tfjs-converter": "~3.7.0",
    "@tensorflow/tfjs-core": "~3.7.0",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-router-dom": "^5.1.7",
    "bootstrap": "^5.0.2",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "seedrandom": "3.0.5",
    "stats.js": "^0.17.0",
    "typescript": "^4.3.2",
    "argparse": "^1.0.10"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:server": "cd ./server && yarn start",
    "build": "react-scripts build && mv -v build/* ./server/public",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "cd ./server && yarn install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/stats.js": "^0.17.0",
    "prettier": "^2.3.1"
  }
